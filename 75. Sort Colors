class Solution:
    def sortColors(self, nums: List[int]) -> None:
        low, i, high = 0, 0, len(nums) - 1
        
        while i <= high:
            if nums[i] == 0:
                # Swap nums[i] with nums[low], increment both i and low
                nums[i], nums[low] = nums[low], nums[i]
                low += 1
                i += 1
            elif nums[i] == 2:
                # Swap nums[i] with nums[high], decrement high (i stays the same)
                nums[i], nums[high] = nums[high], nums[i]
                high -= 1
            else:
                # If nums[i] is 1, just move to the next element
                i += 1
